{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "wom-v2",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": ".*"
      },
      "window": {
        "all": true
      },
      "globalShortcut": {
        "all": true
      },
      "path": {
        "all": true
      },
      "fs": {
        "all": false,
        "scope": ["**"],
        "exists": true,
        "readDir": true
      },
      "clipboard": {
        "all": true
      },
      "notification": {
        "all": true
      },
      "dialog": {
        "all": false,
        "ask": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "cc.wom",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "systemTray": {
      "iconPath": "icons/icon.ico",
      "iconAsTemplate": true
    },
    "windows": [
    ]
  }
}
